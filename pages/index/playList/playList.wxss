.personalized_box {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.personalized_menu {
  width: 100%;
  margin: 20rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: center;
}

.personalized_list {
  margin-top: 30rpx;
}

.personalized_img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
}

.personalized_title {
  font-size: 20rpx;
  color: #333;
  width: 200rpx;
}